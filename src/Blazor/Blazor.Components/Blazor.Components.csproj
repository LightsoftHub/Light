    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
	<RootNamespace>Light.Blazor</RootNamespace>
	<AssemblyName>Lightsoft.Blazor.Components</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
	<PackageReference Include="Lightsoft.Extensions" />
	<PackageReference Include="Lightsoft.FileGenerator" />
	<PackageReference Include="Lightsoft.Result" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" />
  </ItemGroup>

</Project>
